{
  "name": "@jonatns/labcoat",
  "version": "0.6.0",
  "description": "Smart contract development toolkit for Alkanes on Bitcoin",
  "type": "module",
  "types": "dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "labcoat": "dist/cli.cjs"
  },
  "scripts": {
    "build": "tsup",
    "postbuild": "chmod +x dist/cli.cjs",
    "prepublishOnly": "npm run build",
    "release": "npm run build && changeset version && changeset publish"
  },
  "files": [
    "dist",
    "templates"
  ],
  "keywords": [
    "bitcoin",
    "alkanes",
    "smart-contracts",
    "wasm",
    "rust",
    "blockchain"
  ],
  "author": "Jonathan P. Navarrete jonatns@gmail.com",
  "license": "MIT",
  "dependencies": {
    "alkanes": "git+https://github.com/kungfuflex/alkanes.git",
    "commander": "^13.1.0",
    "dotenv": "^17.2.3",
    "nanoid": "^5.1.6",
    "ora": "^9.0.0",
    "oyl-sdk": "git@github.com:jonatns/oyl-sdk.git"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  }
}
