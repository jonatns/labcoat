{
  "name": "@jonatns/labcoat",
  "version": "0.6.0",
  "description": "Smart contract development toolkit for Alkanes on Bitcoin",
  "type": "module",
  "author": "Jonathan P. Navarrete <jonatns@gmail.com>",
  "license": "MIT",
  "main": "dist/sdk/index.js",
  "types": "dist/sdk/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk/index.d.ts",
      "import": "./dist/sdk/index.js",
      "require": "./dist/sdk/index.cjs"
    }
  },
  "bin": {
    "labcoat": "dist/cli/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "postbuild": "chmod +x dist/cli/index.js",
    "prepublishOnly": "bun run build",
    "release": "bun run build && changeset version && changeset publish",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [
    "bitcoin",
    "alkanes",
    "smart-contracts",
    "wasm",
    "rust",
    "blockchain"
  ],
  "dependencies": {
    "@jonatns/labcoat": "link:../../Library/pnpm/global/5/node_modules/@jonatns/labcoat",
    "@oyl/sdk": "git+https://github.com/jonatns/oyl-sdk.git",
    "adm-zip": "^0.5.16",
    "alkanes": "git+https://github.com/kungfuflex/alkanes.git",
    "commander": "^13.1.0",
    "dotenv": "^17.2.3",
    "nanoid": "^5.1.6",
    "ora": "^9.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
